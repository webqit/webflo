    "@webqit/oohtml-ssr": "^2.1.1",
    "@webqit/util": "^0.8.11",
    "esbuild": "^0.14.38",
    "ioredis": "^5.5.0",
    "jsdom": "^21.1.1",
    "mime-types": "^2.1.33",
    "pg": "^8.13.3",
    "simple-git": "^2.20.1",
    "stream-slice": "^0.1.2",
    "urlpattern-polyfill": "^4.0.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "chai": "^4.3.6",
