  "keywords": [
    "git-workflow",
    "self-hosting",
    "automatic-deployment",
    "command-line-interface",
    "progressive-web-apps",
    "single-page-apps",
    "vanila-javascript"
  ],
  "homepage": "https://webqit.io/tooling/webflo",
